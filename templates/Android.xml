<templateSet group="Android">
  <template name="id" value="Log.d(&quot;$M_className$&quot;, &quot;$END$&quot;)" description="Log.d(TAG, message)" toReformat="false" toShortenFQNames="true">
    <variable name="M_className" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN_CLASS" value="true" />
      <option name="KOTLIN_COMMENT" value="true" />
      <option name="KOTLIN_EXPRESSION" value="true" />
      <option name="KOTLIN_OBJECT_DECLARATION" value="true" />
      <option name="KOTLIN_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>